    "@storybook/react": "^3.4.11",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "7.1.3",
    "babel-minify": "^0.3.0",
    "babel-plugin-add-module-exports": "^0.2.1",
