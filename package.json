{
  "name": "react-pretty-icons",
  "version": "0.0.12",
  "description": "Lightweight pack of svg icons for React projects",
  "main": "dist/index.js",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "___prepublish__": "chmod +x ./tools/*;./tools/build",
