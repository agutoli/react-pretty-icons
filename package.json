    "eslint-config-standard": "6.2.1",
    "jsdom": "9.11.0",
    "mocha": "3.2.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "s3": "4.4.0",
    "svg-resizer": "0.0.1",
